<div class= "container">
    <form action = "/create" method = "POST">
      <br>
      <div class="form-group">
        <label for="inputTitle">제목</label>
        <input type="text" name = "title" class="form-control" id="inputTitle" aria-describedby="emailHelp" placeholder="제목을 입력해주세요">
      </div>
      <div class="form-group">
        <label for="inputContent">내용</label>
        <textarea class="form-control" name = "content" rows="5" id="comment" placeholder= "내용을 입력해주세요"></textarea>
      </div>
      <div class="form-group">
        <label for="inputEditor">작성자</label>
        <input type="text" name = "editor" class="form-control" id="inputEditor" aria-describedby="emailHelp" placeholder="이름을 입력해주세요">
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>